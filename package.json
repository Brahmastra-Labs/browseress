  },
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "lint": "eslint .",
    "test": "mocha --exclude 'test/harness/**' --exclude 'test/e2e/**'",
    "test:harness": "playwright test test/harness/simple-test-runner.spec.js",
    "test:unit": "mocha --exclude 'test/harness/**' --exclude 'test/e2e/**'",
    "test:integration": "mocha --require test/support/env --reporter spec --check-leaks test/integration.*.js",
    "test:e2e": "playwright test test/e2e/",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:generate-manifest": "node scripts/generate-test-manifest.js",
    "test-ci": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=lcovonly --reporter=text npm test",
    "test-cov": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=html --reporter=text npm test",
    "test-tap": "mocha --require test/support/env --reporter tap --check-leaks test/ test/acceptance/",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install"
  }
}
